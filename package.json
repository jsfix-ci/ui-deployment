{
  "name": "@simpleer/ui-deployment",
  "version": "0.0.7",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@angular/cli": "^11.1.2"
  },
  "bin": {
    "simpleer-deploy": "dist/cli"
  },
  "scripts": {
    "setup": "cat gitlab.npmrc > .npmrc && echo \"//gitlab.jteq.dev/api/v4/projects/${CI_PROJECT_ID}/packages/npm/:_authToken=${CI_JOB_TOKEN}\" >> .npmrc",
    "build": "tsc",
    "postbuild": "cp src/cli dist/cli",
    "prepublish": "npm version patch && git push --tags origin HEAD:master"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.22",
    "azure-storage": "^2.10.3",
    "commander": "^7.0.0",
    "lodash": "^4.17.20",
    "mime-types": "^2.1.28",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  }
}
